<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: 000000;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.comp485project.AccountController">
   <children>
      <Button fx:id="toDashboard" layoutX="24.0" layoutY="39.0" mnemonicParsing="false" onAction="#switchToDashboard" text="Back to Dashboard">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <Text layoutX="341.0" layoutY="67.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Account">
         <font>
            <Font name="Times New Roman" size="35.0" />
         </font>
      </Text>
      <Text fx:id="displayName" layoutX="370.0" layoutY="177.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Text>
      <Text fx:id="displayEmail" layoutX="326.0" layoutY="222.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Email Address">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Text>
      <Text fx:id="displayPhoneNumber" layoutX="325.0" layoutY="261.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Phone Number">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Text>
      <Text fx:id="displayUsername" layoutX="349.0" layoutY="293.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username">
         <font>
            <Font name="Times New Roman" size="25.0" />
         </font>
      </Text>
      <Button fx:id="changePassword" layoutX="323.0" layoutY="430.0" mnemonicParsing="false" text="Change Password">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
      <Button fx:id="deleteAccount" layoutX="36.0" layoutY="501.0" mnemonicParsing="false" text="Delete Account" textFill="#df1b1b">
         <font>
            <Font name="Times New Roman" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
